<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="launcher" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Windows">
				<Option output="../bridgecommand" prefix_auto="1" extension_auto="1" />
				<Option working_dir=".." />
				<Option type="1" />
				<Option compiler="gcc" />
				<Linker>
					<Add library="../libs/Irrlicht/irrlicht-svn/lib/Win32-gcc/libIrrlicht.a" />
					<Add directory="../libs/Irrlicht/irrlicht-svn/include" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c cd .. &amp; useMinGWdll.bat" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="Linux">
				<Option output="../bridgecommand" prefix_auto="1" extension_auto="1" />
				<Option working_dir=".." />
				<Option type="1" />
				<Option compiler="gcc" />
				<Linker>
					<Add option="-L../libs/Irrlicht/irrlicht-svn/lib/Linux" />
					<Add library="Irrlicht" />
					<Add library="GL" />
					<Add library="Xxf86vm" />
					<Add library="Xext" />
					<Add library="X11" />
					<Add library="Xcursor" />
				</Linker>
			</Target>
			<Target title="LinuxForDeb">
				<Option output="../bridgecommand" prefix_auto="1" extension_auto="1" />
				<Option working_dir=".." />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-DFOR_DEB" />
				</Compiler>
				<Linker>
					<Add option="-L../libs/Irrlicht/irrlicht-svn/lib/Linux" />
					<Add library="Irrlicht" />
					<Add library="GL" />
					<Add library="Xxf86vm" />
					<Add library="Xext" />
					<Add library="X11" />
					<Add library="Xcursor" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c++11" />
			<Add option="-fexceptions" />
			<Add directory="../libs/Irrlicht/irrlicht-svn/include" />
		</Compiler>
		<Unit filename="../IniFile.cpp" />
		<Unit filename="../IniFile.hpp" />
		<Unit filename="../Lang.cpp" />
		<Unit filename="../Lang.hpp" />
		<Unit filename="../Utilities.cpp" />
		<Unit filename="../Utilities.hpp" />
		<Unit filename="../icon.rc">
			<Option compilerVar="WINDRES" />
			<Option target="Windows" />
		</Unit>
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
